<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head>    <title>GroupList</title></head><body><table>    <thead>    <td>Nazwa grupy</td>    <td>Ilość graczy</td>    <td></td>    </thead>    <tbody>    <c:forEach items="${betGroups}" var="betGroup">        <tr>            <td>${betGroup.name}</td>            <td>${betGroup.userGroupAccountList.size()}</td>            <td><button><a href="/betGroup/userList/${betGroup.id}">Wejdź</a></button></td>        </tr>    </c:forEach>    </tbody></table><button><a href="/betGroup/add">Dodaj nową grupę</a></button><button><a href="/">Powrót</a></button></body></html>