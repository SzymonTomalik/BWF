<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head>    <title>Users Groups List</title></head><body><c:choose>    <c:when test="${allUserGroups==null||allUserGroups.size()==0}">        <p>Nie należysz jeszcze do zadnej grupy</p>    </c:when>    <c:when test="${allUserGroups.size()==1}">        <c:redirect url="/betGroup/userList/${allUserGroups.get(0).betGroup.id}"/>    </c:when>    <c:otherwise>        <table>            <thead>            <td>Twoje grupy:</td>            <td></td>            </thead>            <tbody>            <c:forEach items="${allUserGroups}" var="group">                <tr>                    <td>${group.betGroup.name}</td>                    <td>                        <button><a href="/betGroup/userList/${group.betGroup.id}">Wejdź</a></button>                    </td>                </tr>            </c:forEach>            </tbody>        </table>    </c:otherwise></c:choose><button><a href="/">Powrót do menu</a></button></body></html>