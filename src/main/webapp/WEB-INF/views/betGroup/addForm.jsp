<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><html><head>    <title>New BetGroup Form</title></head><body><form:form modelAttribute="betGroup" method="post" action="/betGroup/add" enctype="utf8">    <form:label path="name">Nazwa grupy</form:label>    <form:input path="name"/><br>    <form:errors path="name" cssStyle="color: red"/><br>    <form:label path="groupPassword">Hasło grupy</form:label>    <form:password path="groupPassword"/><br>    <form:errors path="groupPassword" cssStyle="color: red"/><br>    <button type="submit">UTWÓRZ GRUPĘ</button>    <button><a href="${pageContext.request.contextPath}/betGroup/list">Powrót</a></button></form:form></body></html>