<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><html><head>    <title>JoinBetGroupForm</title></head><body><form:form modelAttribute="betGroup" method="post" action="/betGroup/join/${betGroup.id}" enctype="utf8">    <form:label path="name">Dołączasz do grupy</form:label>    <form:label path="name">${betGroup.name}</form:label><br>    <form:label path="groupPassword">Podaj hasło grupy</form:label>    <form:password path="groupPassword"/><br>    <form:errors path="groupPassword" cssStyle="color: red"/><br>    <button type="submit">Dołącz</button>    <button><a href="/">Powrót do menu</a></button></form:form></body></html>