<%@ page import="pl.coderslab.BWF.entity.UserGroupAccount" %><%@ page import="pl.coderslab.BWF.services.UserGroupAccountService" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head>    <title>GroupUsersList</title></head><body><table>    <thead>    <td>Gracz</td>    <td>Ilość punktów</td>    </thead>    <tbody>    <c:forEach items="${groupUsers}" var="user">        <tr>            <td>${user.login}</td>            <c:forEach items="${userGroupAccountList}" var="uga">                <c:if test="${uga.user.equals(user)}">                    <td>${uga.betsPoints}</td>                </c:if>            </c:forEach>        </tr>    </c:forEach>    </tbody></table><td><button><a href="/betGroup/join/${id}">Dołącz</a></button></td><button><a href="/">Powrót do menu</a></button></body></html>