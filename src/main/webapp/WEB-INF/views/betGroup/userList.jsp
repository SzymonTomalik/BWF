<%@ page import="pl.coderslab.BWF.entity.UserGroupAccount" %><%@ page import="pl.coderslab.BWF.services.UserGroupAccountService" %><%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head>    <title>GroupUsersList</title></head><body><h3>Nazwa grupy: ${betGroup.name}</h3><c:choose>    <c:when test="${groupUsers.size()==0}">        <p>W tej grupie nie ma jeszcze żadnych graczy.</p>    </c:when>    <c:otherwise>        <table>            <thead>            <td>Gracz</td>            <td>Ilość punktów</td>            </thead>            <tbody>            <c:forEach items="${groupUsers}" var="user">                <tr>                    <td>${user.login}</td>                    <c:forEach items="${userGroupAccountList}" var="uga">                        <c:if test="${uga.user.equals(user)}">                            <td align="center">${uga.betsPoints}</td>                        </c:if>                    </c:forEach>                </tr>            </c:forEach>            </tbody>        </table>    </c:otherwise></c:choose><br><c:if test="${!groupUsers.contains(user)}">    <td>        <button><a href="/betGroup/join/${id}">Dołącz</a></button>    </td>    <br></c:if><c:if test="${groupUsers.contains(user)}">    <td>        <button><a href="/scoreTypes/myTypes/${id}">Twoje typy</a></button>    </td>    <br></c:if><br><button><a href="/">Powrót do menu</a></button></body></html>