<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><html><head>    <title>My Types in group ${betGroup.name}</title></head><body><h3>Moje typy w grupie: ${betGroup.name}</h3><c:choose>    <c:when test="${myTypesList.size()==0}">        <p style="color: red">W tej grupie nie oddałeś jeszcze żadnych typów.</p>    </c:when>    <c:otherwise>        <table>            <thead>            <td></td>            <td align="center">Drużyna Gospodarzy</td>            <td></td>            <td align="center">Drużyna Gości</td>            <td></td>            </thead>            <tbody>            <tr></tr>            <c:forEach items="${myTypesList}" var="myType">                <tr>                    <td>Mecz: ${myType.soccerMatch.id}</td>                    <td align="center">${myType.soccerMatch.homeTeam}</td>                    <td></td>                    <td align="center">${myType.soccerMatch.awayTeam}</td>                    <td></td>                </tr>                <tr>                    <td>Wynik spotkania:</td>                        <%--                    <c:if test="${myType.soccerMatch.matchDate.>=}"--%>                    <td align="right">${myType.soccerMatch.homeTeamScore}</td>                    <td align="center"> :</td>                    <td align="left">${myType.soccerMatch.awayTeamScore}</td>                    <td></td>                </tr>                <tr>                    <td>Mój typ:</td>                    <td align="right">${myType.typedHomeTeamResult}</td>                    <td align="center"> :</td>                    <td align="left">${myType.typedAwayTeamResult}</td>                    <c:choose>                        <c:when test="${changeableTypes.contains(myType)}">                            <td>                                <button><a href="/scoreTypes/edit/scoreTypeId=${myType.id}"/> Edytuj Typ</button>                            </td>                        </c:when>                        <c:otherwise>                            <td align="center">edycja niemożliwa</td>                        </c:otherwise>                    </c:choose>                </tr>                <tr>                    <td></td>                    <td></td>                    <td></td>                    <td></td>                </tr>            </c:forEach>            </tbody>        </table>    </c:otherwise></c:choose><button><a href="/">Powrót do menu</a></button></body></html>