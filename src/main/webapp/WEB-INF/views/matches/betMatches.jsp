<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="input" uri="http://www.springframework.org/tags/form" %><html><head>    <title>BetMatchesForm</title></head><body><form:form modelAttribute="typeMatchForm" method="post" action="/matches/betMatches" enctype="utf8">    <c:choose>        <c:when test="${betGroups.size()>1}">            <p>Wybierz z listy grupę, w której chcesz wytypować mecze:            <form:select path="betGroupId">                <form:options items="${betGroups}" itemValue="id" itemLabel="name"/>            </form:select>            </p>        </c:when>        <c:otherwise>            <input name="betGroupId" readonly="true" value="${betGroups.get(0)}"/>        </c:otherwise>    </c:choose>    <table>        <tr>            <th>No.</th>            <th>Drużyna gospodarzy</th>            <th></th>            <th></th>            <th></th>            <th>Drużyna gości</th>        </tr>        <c:forEach items="${soccerMatchList}" var="soccerMatch" varStatus="i">            <tr>                <td>                    <label>                        <input readonly name="typeMatchList[${i.index}].matchId" type="text" value="${soccerMatch.id}"><br>                        <form:errors path="typeMatchList[${i.index}].matchId" cssStyle="color: red"/><br>                    </label>                </td>                <td>${soccerMatch.homeTeam}</td>                <td><label>                    <form:input path="typeMatchList[${i.index}].homeTeamResult"/>                    <form:errors path="typeMatchList[${i.index}].homeTeamResult" cssStyle="color: red"/><br>                </label></td>                <td>:</td>                <td><label>                    <form:input path="typeMatchList[${i.index}].awayTeamResult"/>                    <form:errors path="typeMatchList[${i.index}].awayTeamResult" cssStyle="color: red"/><br>                </label></td>                <td>${soccerMatch.awayTeam}</td>            </tr>        </c:forEach>    </table>    <button type="submit">Wyślij typy</button></form:form><button><a href="/">Powrót do menu</a></button></body></html>