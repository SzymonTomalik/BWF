<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="input" uri="http://www.springframework.org/tags/form" %><html><head>    <title>BetMatchesForm</title></head><body><form:form modelAttribute="typeMatchForm" method="post" action="/matches/betMatches" enctype="utf8">    <c:choose>        <c:when test="${betGroups.size()>1}">            <form:select path="betGroupId">                <form:option value="" label="Wybierz grupę w której chcesz wytypować mecze"/>                <%--            <c:forEach items="${betGroups}" var="betGroup">--%>                <form:options items="${betGroups}" itemValue="id" itemLabel="name"/>                <%--            </c:forEach>--%>            </form:select>        </c:when>        <c:otherwise>            <input type="hidden" id="userAccount" name="userAccount" value="${betGroups.get(0)}">        </c:otherwise>    </c:choose>    <table>        <tr>            <th>No.</th>            <th>Drużyna gospodarzy</th>            <th></th>            <th></th>            <th></th>            <th>Drużyna gości</th>        </tr>        <c:forEach items="${soccerMatchList}" var="soccerMatch">            <tr>                <td><label>                    <input readonly name="matchId" value="${soccerMatch.id}">                </label></td>                <td>${soccerMatch.homeTeam}</td>                <td><label>                    <input name="homeTeamResult">                </label></td>                <td>:</td>                <td><label>                    <input name="awayTeamResult">                </label></td>                <td>${soccerMatch.awayTeam}</td>            </tr>        </c:forEach>    </table></form:form><%--    </table>--%><%--            <form:label readonly="true" path="matchId">${}</form:label>--%>