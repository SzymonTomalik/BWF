<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="input" uri="http://www.springframework.org/tags/form" %><html><head>    <title>BetMatchesForm</title></head><body><form:form modelAttribute="typeMatchForm" method="post" action="/matches/betMatches" enctype="utf8">    <c:choose>        <c:when test="${betGroups.size()>1}">            <p>Wybierz z listy grupę, w której chcesz wytypować mecze:</p>            <form:select path="betGroupId" >                <form:options items="${betGroups}" itemValue="id" itemLabel="name"/>            </form:select>        </c:when>        <c:otherwise>            <input type="hidden" id="userAccount" name="userAccount" value="${betGroups.get(0)}">        </c:otherwise>    </c:choose>    <table>        <tr>            <th>No.</th>            <th>Drużyna gospodarzy</th>            <th></th>            <th></th>            <th></th>            <th>Drużyna gości</th>        </tr>        <c:forEach items="${soccerMatchList}" var="soccerMatch" varStatus="status">            <tr>                <td><label>                    <input readonly name="${typeMatchForm.typeMatchList[status.index].matchId}" value="${soccerMatch.id}">                </label></td>                <td>${soccerMatch.homeTeam}</td>                <td><label>                    <input name="${typeMatchForm.typeMatchList[status.index].homeTeamResult}">                </label></td>                <td>:</td>                <td><label>                    <input name="${typeMatchForm.typeMatchList[status.index].awayTeamResult}">                </label></td>                <td>${soccerMatch.awayTeam}</td>            </tr>        </c:forEach>    </table>    <button type="submit">Wyślij typy</button></form:form><button><a href="/">Powrót do menu</a></button></body></html>