<%@ page contentType="text/html;charset=UTF-8" language="java" %><%@ taglib prefix="form" uri="http://www.springframework.org/tags/form" %><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="input" uri="http://www.springframework.org/tags/form" %><html><head>    <title>BetMatchesForm</title></head><body><form:form modelAttribute="scoreType" method="post" action="/matches/betMatches" enctype="utf8">    <c:if test="${betGroups.size()>1}">        <form:select path="userAccount">            <form:option value="NONE" label="Wybierz grupę w której chcesz wytypować mecze"/>            <form:options items="${betGroups}" itemValue="${betGroup.name}" itemLabel="Grupa"/>        </form:select>    </c:if>    <c:forEach items="${soccerMatchList}" var="soccerMatch">        <form:input readonly="true" path="soccerMatchId">${soccerMatch.id}</form:input>        <form:label path="typedHomeTeamResult">${soccerMatch.homeTeam}</form:label>        <form:input path="typedHomeTeamResult"/><br>        <form:errors path="typedHomeTeamResult" cssStyle="color: red"/><br>        <form:label path="typedAwayTeamResult">${soccerMatch.awayTeam}</form:label>        <form:input path="typedAwayTeamResult"/><br>        <form:errors path="typedAwayTeamResult" cssStyle="color: red"/><br>        <button type="submit">Zapisz Typ</button>        <button><a href="${pageContext.request.contextPath}/">Powrót</a></button>    </c:forEach></form:form></body></html>