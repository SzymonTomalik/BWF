<%@ page contentType="text/html;charset=UTF-8"%><%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core" %><%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt" %><html><head>    <title>MatchList</title></head><body><a href="<c:url value="/matches/betMatches"/>"><button>Wytypuj nadchodzące mecze</button></a><button><a href="/">Powrót do menu</a></button><table>    <thead>    <td>Drużyna Gospodarzy</td>    <td></td>    <td></td>    <td>Wynik</td>    <td>Drużyna Gości</td>    <td>Data spotkania</td>    </thead>    <tbody>    <c:forEach items="${soccerMatchList}" var="soccerMatch">        <tr>            <td>${soccerMatch.homeTeam}</td>            <c:choose>                <c:when test="${soccerMatch.matchDate lt date}">                    <td>${soccerMatch.homeTeamScore} </td>                    <td>:</td>                    <td>${soccerMatch.awayTeamScore} </td>                </c:when>                <c:otherwise>                    <td></td>                    <td>:</td>                    <td></td>                </c:otherwise>            </c:choose>            <td>${soccerMatch.awayTeam}</td>            <td>${soccerMatch.matchDate}</td>        </tr>    </c:forEach>    </tbody></table><button><a href="/">Powrót do menu</a></button></body></html>